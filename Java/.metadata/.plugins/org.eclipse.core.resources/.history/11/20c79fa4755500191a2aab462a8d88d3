package exemplo08;

public class Principal {

	public static void main(String[] args) {
		
		/*
			Regras de cálculo
			1) Divisão
			2) Multiplicação
			3) Subtração
			4) Adição
		*/
				
		//Condicional Composta
		double nota1 = 8.5;
		double nota2 = 7.5;
		double media = (nota1 + nota2) / 2;
		
		System.out.println(media);

	}

}
